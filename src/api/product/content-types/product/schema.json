{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "gender": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gender.gender",
      "inversedBy": "products"
    },
    "size": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::size.size",
      "inversedBy": "products"
    },
    "uniqueID": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    }
  }
}
